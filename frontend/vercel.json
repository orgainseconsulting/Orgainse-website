{
  "version": 2,
  "name": "orgainse-consulting",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/about-us-1",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/contactus",
      "destination": "/contact", 
      "permanent": true
    },
    {
      "source": "/shop/:path*",
      "destination": "/services",
      "permanent": true
    },
    {
      "source": "/helpdesk/:path*",
      "destination": "/contact",
      "permanent": true
    },
    {
      "source": "/profile/:path*",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/forum/:path*",
      "destination": "/",
      "permanent": true
    }
  ]
}